# this tests the -° and lteq intrinsic of the DFA

num_test(N type : num.wrap_around) =>
  if N.min -° 1 <= 0 then compile_time_panic else unit

num_test i8
num_test i16
num_test i32
num_test i64
num_test u8
num_test u16
num_test u32
num_test u64
