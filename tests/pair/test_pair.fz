test_pair is
  pair(A,B type, a A, b B) is

  pi i32 := 3;
  pl i64 := 5;
  p1 pair i32 i64 := pair i32 i64 pi pl

  say "p1.a = {p1.a}"
  say "p1.b = {p1.b}"
