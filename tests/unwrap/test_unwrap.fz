test_unwrap =>

  a := mut.new 3
  b := concur.atomic 5

  say2(v i32) => say v

  (try (option i32)).on ()->
    (try (outcome i32)).on ()->
      say a
      say b

      say2 a
      say2 b

      e := 0+a+b
      # NYI: UNDER DEVELOPMENT: Feature not found: 'infix +' (one argument), Target feature: 'mutate.new'
      # e := a+b+c+d
      say2 e
