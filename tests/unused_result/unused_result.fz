  a is
  b => 3

  a # should flag an error
  b # should flag an error

  if true then 42 else -1 # should flag an error

  match true # should flag an error: incompatible branches
    f false_ => f
    t true_  => t

  match true # should flag an error
    f false_ => true
    t true_  => true

  unit
