public b : a is

  # reuse hello without redef, hello is private in a
  public hello unit =>
    say "hello from b"

  public hello3 unit =>
    hello2 # calling hello2 visible only in module

  public ret_a a =>
    a

  public ret_b b.this =>
    b.this

  public redef hello_a unit =>
    say "hello_a from b"

  priv is
    public redef as_string String => "priv"

  public get_any Any =>
    priv

  public field i32 := 1
