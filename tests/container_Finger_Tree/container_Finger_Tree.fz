container_Finger_Tree =>

  ft0 := container.Finger_Tree i32 .empty
  ft1 := ft0 ++ [4,5]
  ft2 := [1,2,3] ++ ft1
  ft3 := ft2 ++ [6,7,8]

  ft4 :=
    for r := ft3, r ++ [x]
        x in 9..20 do
    else
      r

  ft5 :=
    for r := ft3, [-x] ++ r
        x in 0..10 do
    else
      r

  say ft1
  say ft1[0]
  say ft1[ft1.count-1]
  say ft1.count

  say ft2
  say ft2[0]
  say ft2[ft2.count-1]
  say ft2.count

  say ft3
  say ft3[0]
  say ft3[ft3.count-1]
  say ft3.count

  say ft4
  say ft4[0]
  say ft4[ft4.count-1]
  say ft4.count

  say ft5
  say ft5[0]
  say ft5[ft5.count-1]
  say ft5.count

