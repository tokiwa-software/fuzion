num_testm(N type : num.wrap_around) =>
  if N.min -° 1 <= 0 then compile_time_panic else unit
num_testp(N type : num.wrap_around) =>
  if N.max +° 1 <= 0 then unit else compile_time_panic


num_testm i8
num_testm i16
num_testm i32
num_testm i64
num_testm u8
num_testm u16
num_testp u32

