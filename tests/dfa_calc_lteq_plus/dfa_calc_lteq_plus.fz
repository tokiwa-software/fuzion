# this tests the +° and lteq intrinsic of the DFA

num_testp(N type : num.wrap_around) =>
  if N.max +° 1 <= 0 then unit else compile_time_panic


num_testp i8
num_testp i16
num_testp i32
num_testp i64
num_testp u8
num_testp u16
num_testp u32
num_testp u64
